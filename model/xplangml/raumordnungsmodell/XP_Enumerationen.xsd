	<xs:simpleType name="XP_ZweckbestimmungKennzeichnung">
		<xs:annotation>
			<xs:documentation>Aufzählung der Kennzeichnungen nach §5, Abs. 3 BauGB</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:&lt;undefined&gt;">
			<xs:enumeration value="Naturgewalten">
				<xs:annotation>
					<xs:documentation>Flächen, bei deren Bebauung besondere bauliche Sicherungsmaßnahmen gegen Naturgewalten erforderlich sind (§5, Abs. 3, Nr. 1 BauGB).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Abbauflaeche">
				<xs:annotation>
					<xs:documentation>Flächen, unter denen der Bergbau umgeht oder die für den Abbau von Mineralien bestimmt sind (§5, Abs. 3, Nr. 2 BauGB).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AeussereEinwirkungen">
				<xs:annotation>
					<xs:documentation>Flächen, bei deren Bebauung besondere bauliche Sicherungsmaßnahmen gegen äußere Einwirkungen erforderlich sind (§5, Abs. 3, Nr. 1 BauGB).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SchadstoffBelastBoden">
				<xs:annotation>
					<xs:documentation>Für bauliche Nutzung vorgesehene Flächen, deren Böden erheblich mit umweltgefährdenden Stoffen belastet sind (§5, Abs. 3, Nr. 3 BauGB).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LaermBelastung">
				<xs:annotation>
					<xs:documentation>ür bauliche Nutzung vorgesehene Flächen, die erheblichen Lärmbelastung ausgesetzt sind.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Bergbau"/>
			<xs:enumeration value="Bodenordnung"/>
			<xs:enumeration value="AndereGesetzlVorschriften">
				<xs:annotation>
					<xs:documentation>Kennzeichnung nach anderen gesetzlichen Vorschriften.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
