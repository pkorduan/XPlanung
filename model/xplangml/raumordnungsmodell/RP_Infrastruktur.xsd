        <sequence>
          <element maxOccurs="unbounded" minOccurs="0" name="typAE" type="xplan:RP_AbfallentsorgungTypenType">
            <annotation>
              <documentation>Klassifikation von Abfallentsorgung-Infrastruktur.</documentation>
            </annotation>
          </element>
          <element maxOccurs="unbounded" minOccurs="0" name="abfallTyp" type="xplan:RP_AbfallTypenType">
            <annotation>
              <documentation>Klassifikation von mit der Entsorgungsinfrastruktur in Beziehung stehenden Abfalltypen</documentation>
            </annotation>
          </element>
          <element maxOccurs="unbounded" minOccurs="0" name="typAW" type="xplan:RP_AbwasserTypenType">
            <annotation>
              <documentation>Klassifikation von Abwasser-Infrastruktur.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="istAufschuettungAblagerung" type="boolean"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
