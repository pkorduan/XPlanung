	<xs:simpleType name="RP_Rechtscharakter">
		<xs:restriction base="xs:&lt;undefined&gt;">
			<xs:enumeration value="ZielDerRaumordnung">
				<xs:annotation>
					<xs:documentation>Ziel der Raumordnung.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GrundsatzDerRaumordnung">
				<xs:annotation>
					<xs:documentation>Grundsatz der Raumordnung.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NachrichtlicheUebernahme">
				<xs:annotation>
					<xs:documentation>Nachrichtliche Übernahme.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NachrichtlicheUebernahmeZiel">
				<xs:annotation>
					<xs:documentation>Nachrichtliche Übernahme Ziel.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NachrichtlicheUebernahmeGrundsatz">
				<xs:annotation>
					<xs:documentation>Nachrichtliche Übernahme Grundsatz.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NurInformationsgehalt">
				<xs:annotation>
					<xs:documentation>Nur Informationsgehalt.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TextlichesZiel">
				<xs:annotation>
					<xs:documentation>Textliches Ziel.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ZielundGrundsatz">
				<xs:annotation>
					<xs:documentation>Ziel und Grundsatz.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Vorschlag">
				<xs:annotation>
					<xs:documentation>Vorschlag.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="RP_Geometrieobjekt" type="RP_Geometrieobjekt"/>
	<xs:complexType name="RP_Geometrieobjekt" abstract="true">
		<xs:annotation>
			<xs:documentation>Basisklasse für alle Objekte eines Raumordnungsplans mit variablem Raumbezug. Ein konkretes Objekt muss entweder punktförmigen, linienförmigen oder flächenhaften Raumbezug haben, gemischte Geometrie ist nicht zugelassen.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="RP_Objekt">
				<xs:sequence>
					<xs:element name="position" type="XP_VariableGeometrie" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Variabler Raumbezug.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="flaechenschluss" type="xs:boolean" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Zeigt an, ob für das Objekt Flächenschluss vorliegt.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="RP_SonstPlanArt" type="RP_SonstPlanArt"/>
	<xs:complexType name="RP_SonstPlanArt">
		<xs:annotation>
			<xs:documentation>Aufzählung weiterer Arten eines Regionalplans.</xs:documentation>
		</xs:annotation>
		<xs:sequence/>
	</xs:complexType>
	<xs:element name="RP_Status" type="RP_Status"/>
	<xs:complexType name="RP_Status">
		<xs:sequence/>
	</xs:complexType>
	<xs:element name="RP_Bereich" type="RP_Bereich"/>
	<xs:complexType name="RP_Bereich">
		<xs:annotation>
			<xs:documentation>Die Klasse modelliert einen Bereich eines Raumordnungsplans.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="XP_Bereich">
				<xs:sequence>
					<xs:element name="versionBROG" type="Date" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Datum der zugrunde liegenden Version des ROG.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="versionBROGText" type="CharacterString" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Titel der zugrunde liegenden Version des Bundesraumordnungsgesetzes.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="versionLPLG" type="Date" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Datum des zugrunde liegenden Landesplanungsgesetzes.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="versionLPLGText" type="CharacterString" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Titel des zugrunde liegenden Landesplanungsgesetzes.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="geltungsmassstab" type="xs:integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>(Rechtlicher) Geltungsmaßstab des Bereichs.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="rasterAenderung" type="RP_RasterplanAenderung" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="inhaltRPlan" type="RP_Objekt" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
