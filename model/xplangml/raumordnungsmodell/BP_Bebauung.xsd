					<xs:element name="zweckbestimmung" type="BP_ZweckbestimmungGemeinschaftsanlagen" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Zweckbestimmung der Fläche</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="weitereZweckbestimmung1" type="BP_ZweckbestimmungGemeinschaftsanlagen" minOccurs="0" maxOccurs="1">
