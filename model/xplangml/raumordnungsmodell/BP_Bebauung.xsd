	</xs:annotation>
	<xs:include/>
	<xs:include/>
	<xs:include schemaLocation="XP_Enumerationen.xsd"/>
	<xs:include/>
	<xs:include/>
	<xs:include schemaLocation="BP__Basisobjekte.xsd"/>
	<xs:include schemaLocation="XP_Basisobjekte.xsd"/>
	<xs:element name="BP_RegelungVergnuegungsstaetten" type="BP_RegelungVergnuegungsstaetten"/>
	<xs:complexType name="BP_RegelungVergnuegungsstaetten">
		<xs:annotation>
			<xs:documentation>Festsetzung nach §9 Abs. 2b BauGB (Zulässigkeit von Vergnügungsstätten)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BP_Ueberlagerungsobjekt">
				<xs:sequence>
					<xs:element name="zulaessigkeit" type="BP_ZulaessigkeitVergnuegungsstaetten" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Zulässigkeit von Vergnügungsstätten.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BP_AbstandsFlaeche" type="BP_AbstandsFlaeche"/>
	<xs:complexType name="BP_AbstandsFlaeche">
		<xs:annotation>
			<xs:documentation>Festsetzung eines vom Bauordnungsrecht abweichenden Maßes der Tiefe der Abstandsfläche gemäß § 9 Abs 1. Nr. 2a BauGB</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BP_Ueberlagerungsobjekt">
				<xs:sequence>
					<xs:element name="tiefe" type="Length" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Absolute Angabe derTiefe.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BP_DetailZweckbestGemeinschaftsanlagen" type="BP_DetailZweckbestGemeinschaftsanlagen"/>
	<xs:complexType name="BP_DetailZweckbestGemeinschaftsanlagen">
		<xs:sequence/>
	</xs:complexType>
	<xs:element name="BP_GemeinschaftsanlagenZuordnung" type="BP_GemeinschaftsanlagenZuordnung"/>
	<xs:complexType name="BP_GemeinschaftsanlagenZuordnung">
		<xs:annotation>
			<xs:documentation>Zuordnung von Gemeinschaftsanlagen zu Grundstücken.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BP_Geometrieobjekt">
				<xs:sequence>
					<xs:element name="zuordnung" type="BP_GemeinschaftsanlagenFlaeche" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Relation auf die zugeordneten Gemeinschaftsanlagen-Flächen, die außerhalb des Baugebiets liegen.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="BP_GrenzBebauung">
		<xs:annotation>
			<xs:documentation>Aufzählung verschiedener Möglichkeiten, die Bebauung der vorderen, hinteren oder seitlichen Grundstücksgrenzen zu regeln.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:&lt;undefined&gt;">
			<xs:enumeration value="Verboten">
				<xs:annotation>
					<xs:documentation>Eine Bebauung der Grenze ist verboten.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Erlaubt">
				<xs:annotation>
					<xs:documentation>Eine Bebauung der Grenze ist erlaubt.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Erzwungen">
				<xs:annotation>
					<xs:documentation>Eine Bebauung der Grenze ist vorgeschrieben.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="BP_SpezielleBauweise" type="BP_SpezielleBauweise"/>
	<xs:complexType name="BP_SpezielleBauweise">
		<xs:annotation>
			<xs:documentation>Festsetzung der speziellen Bauweise / baulichen Besonderheit eines Gebäudes oder Bauwerks.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BP_Ueberlagerungsobjekt">
				<xs:sequence>
					<xs:element name="typ" type="BP_SpezielleBauweiseTypen" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Typ der speziellen Bauweise.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="sonstTyp" type="BP_SpezielleBauweiseSonstTypen" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Über eine CodeList definierter Typ der speziellen Bauweise.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Bmin" type="Length" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Minimale Breite von Baugrundstücken.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Bmax" type="Length" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Naximale Breite von Baugrundstücken.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Tmin" type="Length" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Minimale Tiefe von Baugrundstücken.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Tmax" type="Length" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximale Tiefe von Baugrundstücken.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BP_PersGruppenBestimmteFlaeche" type="BP_PersGruppenBestimmteFlaeche"/>
	<xs:complexType name="BP_PersGruppenBestimmteFlaeche">
		<xs:annotation>
			<xs:documentation>Fläche, auf denen ganz oder teilweise nur Wohngebäude errichtet werden dürfen, die für Personengruppen mit besonderem Wohnbedarf bestimmt sind (§9, Abs. 1, Nr. 8 BauGB)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BP_Ueberlagerungsobjekt">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BP_SpezielleBauweiseSonstTypen" type="BP_SpezielleBauweiseSonstTypen"/>
	<xs:complexType name="BP_SpezielleBauweiseSonstTypen">
		<xs:annotation>
			<xs:documentation>Über eine ExternalCodeList definierte zusätzliche Typen spezieller Bauweisen.</xs:documentation>
		</xs:annotation>
		<xs:sequence/>
	</xs:complexType>
	<xs:element name="BP_DetailDachform" type="BP_DetailDachform"/>
	<xs:complexType name="BP_DetailDachform">
		<xs:sequence/>
	</xs:complexType>
	<xs:simpleType name="BP_BebauungsArt">
		<xs:annotation>
			<xs:documentation>Aufzählung verschiedener Brbauungs-Arten eines Baugebiets.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:&lt;undefined&gt;">
			<xs:enumeration value="Einzelhaeuser">
				<xs:annotation>
					<xs:documentation>Nur Einzelhäuser zulässig.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Doppelhaeuser">
				<xs:annotation>
					<xs:documentation>Nur Doppelhäuser zulässig.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Hausgruppen">
				<xs:annotation>
					<xs:documentation>Nur Hausgruppen zulässig.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EinzelDoppelhaeuser">
				<xs:annotation>
					<xs:documentation>Nur Einzel- oder Doppelhäuser zulässig.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EinzelhaeuserHausgruppen">
				<xs:annotation>
					<xs:documentation>Nur Einzelhäuser oder Hausgruppen zulässig.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DoppelhaeuserHausgruppen">
				<xs:annotation>
					<xs:documentation>Nur Doppelhäuser oder Hausgruppen zulässig.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Reihenhaeuser"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BP_ZulaessigkeitVergnuegungsstaetten">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Zulaessig">
				<xs:annotation>
					<xs:documentation>Vergnügungsstätten sind generell zulässig.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NichtZulaessig">
				<xs:annotation>
					<xs:documentation>Vergnügungsstätten sind generell nicht zulässig.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AusnahmsweiseZulaessig">
				<xs:annotation>
					<xs:documentation>Vergnügungsstätten sind ausnahmsweise zulässig.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BP_Dachform">
		<xs:annotation>
			<xs:documentation>Aufzählungung verschiedener dachformen.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:&lt;undefined&gt;">
			<xs:enumeration value="Flachdach"/>
			<xs:enumeration value="Pultdach"/>
			<xs:enumeration value="Versetztes Pultdach"/>
			<xs:enumeration value="Satteldach"/>
			<xs:enumeration value="Walmdach"/>
			<xs:enumeration value="Krüppelwalmdach"/>
			<xs:enumeration value="Mansarddach"/>
			<xs:enumeration value="Zeltdach"/>
			<xs:enumeration value="Kegeldach"/>
			<xs:enumeration value="Kuppeldach"/>
			<xs:enumeration value="Sheddach"/>
			<xs:enumeration value="Bogendach"/>
			<xs:enumeration value="Turmdach"/>
			<xs:enumeration value="Mischform"/>
			<xs:enumeration value="Sonstiges"/>
		</xs:restriction>
	</xs:simpleType>
