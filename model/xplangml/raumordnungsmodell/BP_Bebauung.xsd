				<xs:annotation>
					<xs:documentation>Garagen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Spielplatz">
				<xs:annotation>
					<xs:documentation>Spielplatz</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Carport">
				<xs:annotation>
					<xs:documentation>Carport</xs:documentation>
